const baseFee = 68.50;
// Base fee to record Mortgage (first four pages)
let morePages = 2.00
// Each additional page after four is $2.00
let pageCount = 22;
// Total page count of Mortgage
let afterFour = (pageCount - 4);
// Additional pages to be charged at $2.00 each
afterFour *= morePages;
// Multiplies the number of pages after four by the cost of each additional page
let totalFee = baseFee + afterFour;
// Calculates the total fee
console.log(totalFee);
// Prints the total fee charged by ROD
